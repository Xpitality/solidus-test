{
  "executionRoleArn": "arn:aws:iam::934187374409:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/enoteca-naturale-app",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 3000,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "environment": [
        {
          "name": "ADMIN_NAME",
          "value": "Marko Boskovic"
        },
        {
          "name": "ADMIN_EMAIL",
          "value": "marko@xpitality.com"
        },
        {
          "name": "ADMIN_PASSWORD",
          "value": "changeme"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "RAILS_ENV",
          "value": "production"
        },
        {
          "name": "RAILS_LOG_TO_STDOUT",
          "value": "true"
        },
        {
          "name": "RAILS_SERVE_STATIC_FILES",
          "value": "true"
        }
      ],
      "secrets": [
        {
          "valueFrom": "Enoteca-Naturale-App-Rails-Master-Key",
          "name": "RAILS_MASTER_KEY"
        }
      ],
      "memory": 512,
      "memoryReservation": 256,
      "image": "934187374409.dkr.ecr.eu-central-1.amazonaws.com/enoteca-naturale:app-production",
      "essential": true,
      "name": "enoteca-naturale-app"
    }
  ],
  "memory": "512",
  "taskRoleArn": "arn:aws:iam::934187374409:role/ecsTaskExecutionRole",
  "family": "enoteca-naturale-app-task",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256"
}
