<% terms_of_service_page = Alchemy::Page.where(name: 'terms of service').first %>
<% if terms_of_service_page %>
  <% terms_of_service_page.elements.each do |element| %>
    <% cache element do %>
      <%= element_view_for element do |el| %>
        <%= el.render :text %>
        <%#= el.render :image %>
      <% end %>
    <% end %>
  <% end %>

<% end %>
